import java.util.Scanner;
class primefactors
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.println("Number is : ");   
int n=sc.nextInt();
System.out.print("prime factors are : " );
for (int i=2; i<=n;i++) 
{
while (n%i==0)
{
System.out.print(i+" ");
n=n/i;
}
}
}
}
