import java.util.*;
class arrays
{
public static void main(String args[])
{
int s,n[];
Scanner cd=new Scanner(System.in);
System.out.println("Enter array size : ");
s=cd.nextInt();
System.out.println("enter array elements : ");
int[] arr1 = new int[s];
for(int i=0;i<s;i++)
{
arr1[i]=cd.nextInt();
}
for(int i=0;i<s;i++)
{

if(i==s-1)
{
arr1[i]=arr1[s-1]+1;
}
if(arr1[i]==0)
{
arr1[i]=arr1[s-1]+1;
}
System.out.print(" "+arr1[i]);
}
}
